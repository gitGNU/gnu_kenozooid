import kenozooid
import kenozooid.cli as cli

if __name__ == '__main__':
    options, args = cli.parse_options()
    assert len(args) > 0, 'command is expected as first parameter'

    cmd = args[0]
    assert cmd in cli.COMMANDS

    cli.COMMANDS[cmd](args)

